<div id="<%= dom_id user %>">
    <tr>
        <th scope="row"><%= user.id %></th>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= image_tag(user.picture.url, size: "100x100") %></td>
        <td><%= link_to "Show",user_path(user), class:"btn btn-success" %></td>
        <td><%= link_to "Edit",edit_user_path(user), class:"btn btn-danger" %></td>
        <td><%= button_to "Delete",user_path(user), class:"btn btn-danger", method: :delete %></td>
    </tr>
</div>
